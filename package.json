{
  "name": "gulpwordpress",
  "version": "0.1.0",
  "year": "2021",
  "description": "Automated WordPress Theme development environment run by Gulp using Sass, Babel, linters and BrowserSync.",
  "main": "index.js",
  "url": "risdesign.ch",
  "scripts": {
    "install": "gulp firstSetup && composer self-update && composer install && gulp",
    "updater": "sh ./shells/updater.sh",
    "start": "gulp",
    "build": "gulp build --prod",
    "gettableprefix": "sh ./shells/gettableprefix.sh",
    "gettableprefix:stage": "npm run gettableprefix stage",
    "dbexport": "sh ./shells/dbexport.sh",
    "dbexport:local": "local",
    "dbexport:stage": "stage",
    "dbexport:prod": "prod",
    "dbimport": "sh ./shells/dbimport.sh",
    "dbimport:local": "local",
    "dbimport:stage": "stage",
    "dbimport:prod": "prod",
    "deploy": "sh ./shells/deploy.sh",
    "deploy:stage": "stage",
    "deploy:prod": "prod",
    "importRemoteDB-stage": "npm run dbexport stage && gulp getSql --stage && gulp replaceDBStrings --stage=local && npm run dbimport local",
    "importRemoteDB-prod": "npm run dbexport prod && gulp getSql --prod && gulp replaceDBStrings --prod=local && npm run dbimport local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcoris/gulpwordpress.git"
  },
  "keywords": [
    "wordpress",
    "theme",
    "sass",
    "babel",
    "webpack",
    "gulp",
    "BrowserSync"
  ],
  "author": "Marco Ris",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcoris/gulpwordpress/issues"
  },
  "homepage": "https://github.com/marcoris/gulpwordpress#readme",
  "website": "https://risdesign.ch",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "apache-server-configs": "^3.2.1",
    "autoprefixer": "^9.8.6",
    "babel": "^6.23.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "del": "^5.1.0",
    "dotenv": "^8.6.0",
    "easy-ftp": "^0.4.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-banner": "^0.1.3",
    "gulp-bump": "^3.2.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-conventional-changelog": "^2.0.34",
    "gulp-eslint": "^6.0.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-prompt": "^1.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^4.1.0",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "gulp-wp-pot": "^2.4.3",
    "gulp-zip": "^5.1.0",
    "lodash.template": "^4.5.0",
    "nucleus-styleguide": "^1.1.1",
    "webshot": "^0.18.0",
    "yargs": "^15.4.1"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "jquery": "^3.6.0",
    "minimist": ">=1.2.2",
    "popper.js": "^1.16.1"
  }
}
